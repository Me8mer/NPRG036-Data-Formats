@prefix ex: <http://example.org/vocabulary/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix schema: <https://schema.org/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

schema:Thing a owl:Class ;
    owl:equivalentClass owl:Thing .

foaf:Thing a owl:Class ;
    owl:equivalentClass owl:Thing .

rdfs:Resource a owl:Class ;
    owl:equivalentClass owl:Thing .

ex:Owner a rdfs:Class ;
    rdfs:label "Owner"@en ;
    rdfs:subClassOf foaf:Agent ;
    rdfs:comment "An entity with the ownership over another entity"@en .
    
ex:EShop a rdfs:Class ;
    rdfs:label "E-Shop"@en ;
    rdfs:subClassOf schema:WebSite ;
    rdfs:comment "A website operating as an online shop"@en .

ex:Item a rdfs:Class ;
    rdfs:label "Item"@en ;
    rdfs:subClassOf schema:Product ;
    rdfs:comment "A single item for sale in an online shop"@en .

ex:Cart a rdfs:Class ;
    rdfs:label "Cart"@en ;
    rdfs:subClassOf schema:ItemList ;
    rdfs:comment "A collection of items chosen for purchase"@en .

ex:Account a rdfs:Class ;
    rdfs:label "Account"@en ;
    rdfs:subClassOf foaf:OnlineAccount ;
    rdfs:comment "An account in an online shop"@en .

ex:Shopper a rdfs:Class ;
    rdfs:label "Shopper"@en ;
    rdfs:subClassOf foaf:Person ;
    rdfs:comment "A person with an account in an online shop"@en .

ex:password a owl:DatatypeProperty ;
    rdfs:label "password"@en ;
    rdfs:domain foaf:OnlineAccount ;
    rdfs:range xsd:string ;
    rdfs:comment "A password for an account in an online shop"@en .

ex:owns a owl:ObjectProperty ;
    rdfs:label "owns"@en ;
    rdfs:domain foaf:Agent ;
    rdfs:range owl:Thing ;
    rdfs:comment "An Agent owns a Thing"@en .

ex:provides a owl:ObjectProperty ;
    rdfs:label "provides"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range owl:Thing ;
    rdfs:comment "A Thing provides access to another Thing"@en .

ex:contains a owl:ObjectProperty ;
    rdfs:label "contains"@en ;
    rdfs:domain owl:Thing ;
    rdfs:range owl:Thing ;
    rdfs:comment "A Thing contains another Thing"@en .

ex:Corp1 a ex:Owner ;
    foaf:name "Goth Corp"@en ;
    dcterms:identifier "FR33Z3"^^xsd:string .

ex:Corp2 a ex:Owner ;
    foaf:name "Wayne Enterprises"@en ;
    dcterms:identifier "8UTTL3R"^^xsd:string .
    
ex:Corp3 a ex:Owner ;
     foaf:name "Lex Crop"@en ;
     dcterms:identifier "HAT3R_1"^^xsd:string .

ex:Customer1 a ex:Shopper ;
    foaf:firstName "Ferris" ;
    foaf:familyName "Boyle" .

ex:Customer2 a ex:Shopper ;
    foaf:firstName "Bruce" ;
    foaf:familyName "Wayne" .

ex:Customer3 a ex:Shopper ;
    foaf:firstName "Lex" ;
    foaf:familyName "Luthor" .

ex:Shop1 a ex:EShop ;
    foaf:name "Goth Fridges"@en ;
    schema:url <https://www.gothcorp.com/shop/fridges> ;
    schema:image <https://example.org/images/fridge.png> .

ex:Shop2 a ex:EShop ;
    foaf:name "Wayne's Gains"@en ;
    schema:url <https://www.waynesgains.com> ;
    schema:image <https://example.org/images/protein.png> .

ex:Shop3 a ex:EShop ;
    foaf:name "Lex Rides"@en ;
    schema:url <https://www.lexcorp.com/vehicles/shop> ;
    schema:image <https://example.org/images/car.png> .

ex:Shop1Item a ex:Item ;
    foaf:name "Cold Cold Fridge" ;
    schema:price "999.99"^^xsd:double ;
    dcterms:description "Perfect to keep your vegetables fresh."@en ;
    schema:availability <https://schema.org/InStock> ;
    schema:image <https://example.org/images/fridge_front.png> ,
                 <https://example.org/images/fridge_side.png> ,
                 <https://example.org/images/fridge_open.png> .

ex:Shop2Item a ex:Item ;
    foaf:name "Bat-Protein" ;
    schema:price "50.49"^^xsd:double ;
    dcterms:description "Keep out of reach of penguins."@en ;
    schema:availability <https://schema.org/OutOfStock> ;
    schema:image <https://example.org/images/protein_top.png> ,
                 <https://example.org/images/protein_side.png> ,
                 <https://example.org/images/protein_inside.png> .

ex:Shop3Item a ex:Item ;
    foaf:name "SuperVan" ;
    schema:price "10024.99"^^xsd:double ;
    dcterms:description "Made on Earth by humans."@en ;
    schema:availability <https://schema.org/PreOrder> ;
    schema:image <https://example.org/images/van_front.png> ,
                 <https://example.org/images/van_back.png> ,
                 <https://example.org/images/van_leather_seats.png> .

ex:Shop1Cart a ex:Cart ;
    schema:numberOfItems "1"^^xsd:integer ;
    schema:price "999.99"^^xsd:double .

ex:Shop2Cart a ex:Cart ;
    schema:numberOfItems "1"^^xsd:integer ;
    schema:price "50.49"^^xsd:double .

ex:Shop3Cart a ex:Cart ;
    schema:numberOfItems "1"^^xsd:integer ;
    schema:price "10024.99"^^xsd:double .

