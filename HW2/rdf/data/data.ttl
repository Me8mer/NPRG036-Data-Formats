    foaf:accountName "Boyoyoyle" ;
    ex:password "nothing2c" .

ex:Customer2Account a ex:Account ;
    foaf:accountName "BatMan4Ever" ;
    ex:password "AlfredDaButtler" .

ex:Customer3Account a ex:Account ;
    foaf:accountName "SuperLoser" ;
    ex:password "1a1a1A" .

ex:Corp1 ex:owns ex:Shop1 .
ex:Corp2 ex:owns ex:Shop2 .
ex:Corp3 ex:owns ex:Shop3 .

ex:Shop1 ex:provides ex:Shop1Item .
ex:Shop2 ex:provides ex:Shop2Item .
ex:Shop3 ex:provides ex:Shop3Item .

ex:Shop1 ex:provides ex:Customer1Account .
ex:Shop2 ex:provides ex:Customer2Account .
ex:Shop3 ex:provides ex:Customer3Account .

ex:Customer1 ex:owns ex:Customer1Account .
ex:Customer2 ex:owns ex:Customer2Account .
ex:Customer3 ex:owns ex:Customer3Account .

ex:Customer1Account ex:provides ex:Shop1Cart .
ex:Customer2Account ex:provides ex:Shop2Cart .
ex:Customer3Account ex:provides ex:Shop3Cart .

ex:Shop1Cart ex:contains ex:Shop1Item .
ex:Shop2Cart ex:contains ex:Shop2Item .
ex:Shop3Cart ex:contains ex:Shop3Item .